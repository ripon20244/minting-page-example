{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "protocol": {
      "type": "object",
      "properties": {
        "name": {
          "const": "BRC721"
        },
        "version": {
          "type": "string"
        }
      },
      "required": ["name", "version"]
    },
    "type": {
      "const": "inscription"
    },
    "content": {
      "type": "string"
    },
    "contentSignature": {
      "type": "string"
    }
  },
  "required": ["protocol", "type", "content", "contentSignature"]
}
